<?xml version="1.0" encoding="UTF-8"?><record_update sys_domain="global" table="pa_scripts">
    <pa_scripts action="INSERT_OR_UPDATE">
        <arguments>metric_data</arguments>
        <description>Extract breach rate from JSON</description>
        <name>Kokoro.Script.BreachRate</name>
        <script><![CDATA[(function() {
    var jsonStr = current.metric_data;
    if (!jsonStr) return 0;
    try {
        var data = JSON.parse(jsonStr);
        // 提取违规率
        return parseFloat(data.breach_rate) || 0;
    } catch(e) { 
        return 0; 
    }
})();]]></script>
