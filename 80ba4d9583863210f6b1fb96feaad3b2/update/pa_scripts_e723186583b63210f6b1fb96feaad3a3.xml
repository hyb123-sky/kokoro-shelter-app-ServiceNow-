<?xml version="1.0" encoding="UTF-8"?><record_update sys_domain="global" table="pa_scripts">
    <pa_scripts action="INSERT_OR_UPDATE">
        <arguments>metric_data</arguments>
        <description>Extract volunteer utilization from JSON</description>
        <name>Kokoro.Script.VolunteerUtil</name>
        <script><![CDATA[(function() {  // <--- 加上这个头部
    var jsonStr = current.metric_data;
    if (!jsonStr) return 0;
    try {
        var data = JSON.parse(jsonStr);
        // 提取利用率，如果没有则返回0
        return parseFloat(data.volunteer_utilization) || 0;
    } catch(e) { 
        return 0; 
    }
})();  // <--- 加上这个尾部]]></script>
